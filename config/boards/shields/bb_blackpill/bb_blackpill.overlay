/ {
	chosen {
		zmk,kscan = &kscan0;
	};

	kscan0: kscan {
		compatible = "zmk,kscan-gpio-direct";

		label = "KSCAN";

		input-gpios
			= <&blackpill 20 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>
			, <&blackpill 19 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>
			;

	};

};
